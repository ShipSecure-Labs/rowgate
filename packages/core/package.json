{
  "name": "@rowgate/core",
  "version": "0.0.5",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "author": "Simon Koeck <simon@koeck.dev> (https://simonkoeck.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShipSecure-Labs/rowgate.git"
  },
  "homepage": "https://rowgate.com/docs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown -c tsdown.ts",
    "clean": "rimraf dist",
    "dev": "tsdown --watch --ignore-watch .turbo",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "eslint": "^9.11.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "jose": "^5.9.3",
    "path-to-regexp": "^6.3.0",
    "zod": "^4.1.11"
  }
}

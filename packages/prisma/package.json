{
  "name": "@shipsecure/gatekeeper-prisma",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown -c tsdown.ts",
    "clean": "rimraf dist",
    "dev": "tsdown --watch --ignore-watch .turbo -c tsdown.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@shipsecure/gatekeeper-core": "workspace:*"
  },
  "devDependencies": {
    "eslint": "^9.11.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": false
    }
  }
}

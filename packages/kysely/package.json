{
  "name": "@rowgate/kysely",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "author": "Simon Koeck <simon@koeck.dev>",
  "repository": "shipsecure-labs/rowgate",
  "homepage": "https://rowgate.com/docs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown -c tsdown.ts",
    "clean": "rimraf dist",
    "dev": "tsdown --watch --ignore-watch .turbo",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@rowgate/core": "workspace:*"
  },
  "devDependencies": {
    "eslint": "^9.11.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "kysely": ">=0.20.0"
  },
  "peerDependenciesMeta": {
    "kysely": {
      "optional": false
    }
  }
}
